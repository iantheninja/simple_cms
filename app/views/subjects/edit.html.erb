<div class="subject edit">
  <h2>Edit Subject</h2>
  <%= link_to "Subjects list", subjects_path, class: 'back link' %>

  <%= form_for @subject do |f| %>
    
    <%= render partial: 'form', locals: {f: f} %>

    <div class="form-buttons">
      <%= f.submit "Edit Subject" %>
    </div>
  <% end %>
  
</div>